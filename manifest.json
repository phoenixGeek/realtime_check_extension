{
    "manifest_version": 2,
    "name": "RealTimeCheckClass",
    "description": "Check the class if exist or not",
    "version": "1.0.1",
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/jquery-3.4.1.min.js",
                "js/bootstrap.min.js",
                "content-script.js"
            ],
            "css": [
                "css/style.css"
            ]
        }
    ],
    "icons": {
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "img/icon_128.png"
        },
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "popup.html",
        "img/*.svg"
    ],
    "background": {
        "scripts": [
            "background.js",
            "js/jquery-3.4.1.min.js"
        ],
        "persistent": false
    },
    "permissions": [
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}